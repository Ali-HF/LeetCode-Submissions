#include <stdio.h>
#include <string.h>

int i;

int lengthOfLastWord(char* s) {
    int count = 0;


    i =  strlen(s) - 1;

    while (i != 0 && s[i] == ' ')
    {
        i = i -1;
    }

    while (i >= 0 && s[i] != ' ')
    {
        count = count + 1;
        i = i - 1;
    }

    return count;
    
}
