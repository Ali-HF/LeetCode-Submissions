#include <stdio.h>
#include <stdbool.h>
#include <string.h>

#define MAX 10000

char stack[MAX];
int top = -1;

void push(char x) {
    if (top < MAX - 1) {
        stack[++top] = x;
    }
}

char pop() {
    if (top == -1) {
        return '\0';
    }
    return stack[top--];
}

bool isEmpty() {
    return top == -1;
}

bool isMatching(char open, char close) {
    return (open == '(' && close == ')') ||
           (open == '{' && close == '}') ||
           (open == '[' && close == ']');
}

           
bool isValid(char* s) {
    top = -1;
    for (int i = 0; i < strlen(s); i++) {
        char ch = s[i];
        if (ch == '(' || ch == '{' || ch == '[') {
            push(ch);
        } else if (ch == ')' || ch == '}' || ch == ']') {
            if (isEmpty()) return false;
            char topChar = pop();
            if (!isMatching(topChar, ch)) return false;
        } else {
            
        }
    }
    return isEmpty();
}
