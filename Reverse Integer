#include <stdio.h>
#include <limits.h>

int digit = 0;

int reverse(int x){
    int result = 0;

    while (x != 0)
    {
        digit = x % 10;
        x = x / 10;

        if (result > INT_MAX/10 || result < INT_MIN/10)
            return 0;
        
        result = result * 10 + digit;
    }

    return result;



}
